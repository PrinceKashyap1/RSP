{
  "version": 2,
  "builds": [
    {
      "src": "Myproject/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    },
    {
      "src": "staticfiles_build",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build/static"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Myproject/wsgi.py"
    }
  ]
}
