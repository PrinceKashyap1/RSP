{
    "version": 2,
    "builds": [
      {
        "src": "Myproject/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime" : "python3.12"
        }
      },
      {
      "src": "build_files",
      "use": "@vercel/static-build",
      "config": {
          "disDir": "staticfiles_build"
        }
    }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "Myproject/wsgi.py"
      }
    ]
  }
  